{
  "name": "045-api",
  "version": "1.5.0",
  "description": "api service for catalog for apps and solutions",
  "author": "jens hackenberg, gerd sander",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "proto": "copyfiles -u 2 src/proto/**/* dist"
  },
  "dependencies": {
    "@nestjs/common": "6.5.1",
    "@nestjs/core": "6.5.1",
    "@nestjs/microservices": "6.5.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "6.5.1",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/websockets": "6.5.1",
    "docxtemplater": "^3.11.1",
    "dotenv": "^8.1.0",
    "form-data": "^2.3.3",
    "fs": "0.0.1-security",
    "gridfs-stream": "^1.1.1",
    "js-yaml": "^3.12.0",
    "json-2-csv": "^3.5.4",
    "jszip": "^2.6.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongoose": "^5.6.11",
    "nanoid": "^2.1.0",
    "read-pkg": "^4.0.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.2",
    "semver-regex": "^2.0.0",
    "semver-sort": "0.0.4",
    "tar": "^4.4.8",
    "swagger-ui-express": "^4.0.7",
    "typescript": "^3.6.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "6.5.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.118",
    "@types/mongoose": "^5.5.16",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^12.7.3",
    "@types/supertest": "^2.0.5",
    "@types/uuid": "^3.4.4",
    "copyfiles": "^2.1.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.19.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
